<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="800.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/null" fx:controller="com.example.vizsgarremek.listetterem">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <children>
        <HBox spacing="20.0">
            <children>
                <Button fx:id="insertButton" mnemonicParsing="false" onAction="#insertClick" text="insert"/>
                <Button fx:id="updateButton" mnemonicParsing="false" onAction="#updateClick" text="update"/>
                <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteClicck" text="Delete"/>
            </children>
        </HBox>
        <TableView fx:id="etelTabla" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="idCol" minWidth="50.0" prefWidth="75.0" text="#"/>
                <TableColumn fx:id="nameCol" minWidth="50.0" prefWidth="200.0" text="name"/>
                <TableColumn fx:id="leirasCol" minWidth="50.0" prefWidth="200.0" text="leiras"/>
                <TableColumn fx:id="kategoriaCol" minWidth="50.0" prefWidth="200.0" text="kategira"/>
                <TableColumn fx:id="arCol" minWidth="50.0" prefWidth="200.0" text="ar"/>
            </columns>
        </TableView>
    </children>
</VBox>
