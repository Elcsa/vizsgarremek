<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.control.TextField?>
<GridPane hgap="20.0" prefHeight="480.0" prefWidth="640.0" vgap="20.0" xmlns="http://javafx.com/javafx/null"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.vizsgarremek.kajamodosit">
    <columnConstraints>
        <ColumnConstraints halignment="RIGHT" minWidth="75.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES"/>
    </rowConstraints>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <children>
        <Label text="Nev:"/>
        <Label text="leiras:" GridPane.rowIndex="1"/>
        <Label text="kategoria:" GridPane.rowIndex="2"/>
        <Label text="ar:" GridPane.rowIndex="3"/>
        <TextField fx:id="nevField" GridPane.columnIndex="1"/>
        <TextField fx:id="leirasField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <TextField fx:id="kategoriaField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <TextField fx:id="arField" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <Button fx:id="updateButton" mnemonicParsing="false" onAction="#updateClick" prefWidth="10000000.0" text="update" GridPane.columnIndex="2" GridPane.rowIndex="4"/>
    </children>
</GridPane>